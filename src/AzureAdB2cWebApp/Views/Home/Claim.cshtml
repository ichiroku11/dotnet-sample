@using Microsoft.AspNetCore.Authentication
@{
	ViewBag.Title = "Claim";

	var properties = ViewBag.Properties as AuthenticationProperties;
}
<p>@ViewBag.Title</p>

<h3>@nameof(User.Claims)</h3>
<table>
	<tr>
		<th>Type</th>
		<th>Value</th>
	</tr>
	@foreach (var claim in User.Claims) {
		<tr>
			<td>@claim.Type</td>
			<td>@claim.Value</td>
		</tr>
	}
</table>

<h3>@nameof(AuthenticationProperties).@nameof(AuthenticationProperties.Items)</h3>
<table>
	<tr>
		<th>Key</th>
		<th>Value</th>
	</tr>
	@foreach (var (key, value) in properties?.Items) {
		<tr>
			<td>@key</td>
			<td>@value</td>
		</tr>
	}
</table>

<h3>@nameof(AuthenticationProperties).@nameof(AuthenticationProperties.Parameters)</h3>
<table>
	<tr>
		<th>Key</th>
		<th>Value</th>
	</tr>
	@foreach (var (key, value) in properties?.Parameters) {
		<tr>
			<td>@key</td>
			<td>@value</td>
		</tr>
	}
</table>
